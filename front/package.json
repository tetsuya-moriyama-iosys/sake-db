{
  "name": "front",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "codegen": "graphql-codegen --config codegen.ts",
    "fetch-schema": "get-graphql-schema http://localhost:8080/query > schema.graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.12.4",
    "@headlessui/vue": "^1.7.23",
    "@tanstack/vue-query": "^5.52.2",
    "@vee-validate/rules": "^4.13.2",
    "@vue/apollo-composable": "^4.0.2",
    "@vue/apollo-option": "^4.0.0",
    "@vue/compiler-sfc": "3.5",
    "@vue/runtime-core": "3.5",
    "@vueuse/core": "^11.0.3",
    "amazon-paapi": "^1.0.7",
    "apollo-upload-client": "^18.0.1",
    "aws-sdk": "^2.1689.0",
    "axios": "^1.7.2",
    "browser-image-compression": "^2.0.2",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.1.7",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.12",
    "vue-loading-overlay": "^6.0.5",
    "vue-query": "^1.26.0",
    "vue-router": "^4.3.3",
    "vuex": "^4.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/client-preset": "4.5.1",
    "@graphql-codegen/typed-document-node": "^5.0.12",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.1",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/language-core": "^2.0.29",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^24.1.0",
    "lefthook": "^1.7.18",
    "lint-staged": "^15.2.7",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "sass": "^1.80.4",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vite": "^5.3.1",
    "vite-plugin-graphql-loader": "^4.0.4",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "^1.6.0",
    "vue-template-compiler": "^0.1.0",
    "vue-tsc": "^2.0.29"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.19.0"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint --fix --ignore-pattern src/graphQL/auto-generated.ts"
  }
}
